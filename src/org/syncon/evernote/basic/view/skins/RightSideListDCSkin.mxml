		xmlns:view="org.syncon.evernote.basic.view.*" >
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
 			
		]]>
	</fx:Script>

	<fx:Metadata>
		[HostComponent("org.syncon.evernote.basic.view.list_note_list")]
	</fx:Metadata>
	
	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />		
	</s:states>
		
	<s:List
